{
  "name": "nti-lib-store",
  "version": "1.0.1",
  "description": "Flux store super class implementation",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "karma start --single-run --reporters dots,html,junit,coverage",
    "start": "make check",
    "prepublish": "not-in-install && make || in-install",
    "preversion": "make test",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NextThought/nti.lib.store.git"
  },
  "keywords": [
    "ntiids"
  ],
  "author": "nextthought",
  "license": "UNLICENSED",
  "dependencies": {
    "nti-lib-dispatcher": "^1.0.0",
    "nti-util-logger": "^1.0.1",
    "invariant": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.1.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^19.0.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-preset-env": "^1.1.4",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.18.0",
    "eslint-config-nti-codestyle-js": "^1.1.0",
    "in-publish": "^2.0.0",
    "jest": "^19.0.2",
    "jest-junit": "^1.4.0",
    "rollup": "^0.41.5",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^8.0.0",
    "rollup-plugin-json": "^2.0.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/*.spec.js"
    ],
    "coverageDirectory": "reports/coverage",
    "testResultsProcessor": "./node_modules/jest-junit"
  },
  "jest-junit": {
    "output": "reports/test-results/index.xml"
  }
}